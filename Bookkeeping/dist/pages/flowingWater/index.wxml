<block wx:if="{{$taroCompReady}}">
    <block>
        <view class="flowingWater">
            <view class="time">
                <picker mode="date" fields="month" bindchange="anonymousFunc0" value="">
                    <view class="time-color">{{year}}
                        <image src="{{IMGCDNURL + 'leftIcon.png'}}" class="leftIcon"></image>{{mon}}
                        <image class="righticon" src="{{IMGCDNURL + 'rightIcon.png'}}"></image>
                    </view>
                </picker>
            </view>
            <view class="content">
                <view>
                    <block wx:if="{{data.item && data.item.length > 0}}">
                        <view wx:key="$loopState__temp2" bindtap="anonymousFunc1" wx:for="{{loopArray51}}" wx:for-item="v" wx:for-index="i" data-e-tap-so="this" data-e-tap-a-a="{{v._$indexKey}}">
                            <view class="content-list">
                                <view class="content-list-left">
                                    <block wx:if="{{v.$original.click}}">
                                        <view class="content-list-icon-lowerIcon">
                                            <image class="content-list-icon-lowerIcon-image" src="{{IMGCDNURL + 'lowerIcon.png'}}"></image>
                                        </view>
                                    </block>
                                    <block wx:if="{{!v.$original.click}}">
                                        <view class="content-list-icon-rightIcon">
                                            <image class="content-list-icon-rightIcon-image" src="{{IMGCDNURL + 'rightIcon.png'}}"></image>
                                        </view>
                                    </block>
                                    <view>
                                        <view class="content-list-left-week"><text>{{v.$original.day}}</text>{{v.$original.week}}</view>
                                        <view class="content-list-left-time">{{v.$original.month}}</view>
                                    </view>
                                </view>
                                <view class="content-list-right">
                                    <view class="content-list-right-list">
                                        <view class="content-list-right-title">借支</view>
                                        <view class="content-list-right-money">¥{{v.$loopState__temp4}}</view>
                                    </view>
                                    <view>
                                        <view class="content-list-right-title">工钱</view>
                                        <view class="content-list-right-money">¥{{v.$loopState__temp6}}</view>
                                    </view>
                                </view>
                            </view>
                            <block wx:if="{{v.$original.click}}">
                                <view>
                                    <view catchtap="anonymousFunc2" wx:for="{{v.$anonymousCallee__10}}" wx:for-item="val" wx:for-index="__index2" data-e-tap-so="this" data-e-tap-a-a="{{val._$indexKey2}}">
                                        <at-swipe-action compid="{{val.$compid__56}}">
                                            <view wx:key="id" class="content-list-subclass" bindtap="anonymousFunc5" data-e-tap-so="this" data-e-tap-a-a="{{val._$indexKey5}}">
                                                <view class="content-list-subclass-left">
                                                    <block wx:if="{{isCheckOut}}">
                                                        <view>
                                                            <checkbox checked="{{val.$original.checkClick}}" class="checkbox" catchtap="anonymousFunc6" value="{{v.$original.checkClick}}" data-e-tap-so="this" data-e-tap-a-a="{{val._$indexKey6}}"></checkbox>
                                                        </view>
                                                    </block>
                                                    <view class="">
                                                        <view>{{val.$original.workername || '-'}}</view>
                                                        <view class="content-list-subclass-left-title">我在{{val.$original.group_info}}对{{val.$original.workername || '-'}}记了1笔包工</view>
                                                    </view>
                                                </view>
                                                <view class="content-list-subclass-money">¥{{val.$original.money}}></view>
                                            </view>
                                        </at-swipe-action>
                                    </view>
                                </view>
                            </block>
                        </view>
                    </block>
                </view>
                <block wx:if="{{data.item.length === 0}}">
                    <view class="noData-box">
                        <view class="noData">
                            <view>本月您还没有记工哦~</view>
                            <view>点击【去记工】按钮，添加您的记工信息</view>
                            <view class="goRecordWorkBox">
                                <view class="goRecordWork" bindtap="anonymousFunc7">去记工</view>
                            </view>
                        </view>
                    </view>
                </block>
            </view>
            <block wx:if="{{!isCheckOut && data.item.length > 0}}">
                <view class="icon-box" bindtap="anonymousFunc8">
                    <view class="icon-box-icon">
                        <image class="icon-box-icon-image" src="{{IMGCDNURL + 'checkout.png'}}"></image>
                    </view>
                    <view>多选</view>
                </view>
            </block>
            <block wx:if="{{isCheckOut}}">
                <view class="footer-box">
                    <view class="footer-box-left">
                        <checkbox class="checkbox" checked="{{allcheck}}" value="" bindtap="anonymousFunc9"></checkbox>全选</view>
                    <view class="footer-box-flex">
                        <view class="allDel" bindtap="anonymousFunc10">批量删除</view>
                        <view class="close" bindtap="anonymousFunc11">取消</view>
                    </view>
                </view>
            </block>
        </view>
    </block>
</block>