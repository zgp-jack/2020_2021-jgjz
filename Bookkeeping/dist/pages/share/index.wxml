<block wx:if="{{$taroCompReady}}">
    <view class="content">
        <view class="box">
            <view>
                <view class="box-left" wx:for="{{fixedTab}}" wx:for-item="v" wx:for-index="_anonIdx4">
                    <view class="box-border" wx:for="{{v.list}}" wx:for-item="val" wx:for-index="_anonIdx2">
                        <block wx:if="{{!val.type}}">
                            <view class="{{val.default ? 'box-list-default' : 'box-list'}}">
                                <view class="name">{{val.name}}</view>
                                <block wx:if="{{!val.default}}">
                                    <view class="blued">微信对工 ></view>
                                </block>
                            </view>
                        </block>
                        <block wx:if="{{val.type}}">
                            <view class="box-list-type">
                                <block wx:if="{{val.type.hour}}">
                                    <view class="box-list">点工</view>
                                </block>
                                <block wx:if="{{val.type.work}}">
                                    <view class="box-list-bao">包工
                                        <view>（按天记）</view>
                                    </view>
                                </block>
                                <block wx:if="{{val.type.amount}}">
                                    <view class="box-list-bao">包工
                                        <view>（按量记）</view>
                                    </view>
                                </block>
                                <block wx:if="{{val.type.borrow}}">
                                    <view class="box-list">借支</view>
                                </block>
                            </view>
                        </block>
                    </view>
                </view>
            </view>
            <view class="box-scroll">
                <view wx:for="{{tebArr}}" wx:for-item="v" wx:for-index="_anonIdx10">
                    <view class="box-right">
                        <view class="box-border" wx:for="{{v.list}}" wx:for-item="val" wx:for-index="_anonIdx8">
                            <block wx:if="{{!val.type}}">
                                <view class="{{val.default ? 'box-list-default' : 'box-list'}}">{{val.name}}</view>
                            </block>
                            <block wx:if="{{val.type}}">
                                <view class="box-list-bao">
                                    <block wx:if="{{val.name == 0}}">
                                        <view>
                                            <block wx:if="{{val.type.hour.work_time}}">
                                                <view class="box-list-bao">
                                                    <block wx:if="{{val.type.hour.work_time}}">
                                                        <view>{{val.type.hour.work_time}}个工</view>
                                                    </block>
                                                    <block wx:if="{{val.type.hour.over_time}}">
                                                        <view>{{val.type.hour.over_time}}小时</view>
                                                    </block>
                                                </view>
                                            </block>
                                            <block wx:if="{{val.type.work.work_time}}">
                                                <view class="box-list-bao">
                                                    <block wx:if="{{val.type.work.work_time !== ''}}">
                                                        <view>{{val.type.work.work_time}}个工</view>
                                                    </block>
                                                    <block wx:if="{{val.type.work.over_time !== ''}}">
                                                        <view>{{val.type.work.over_time}}小时</view>
                                                    </block>
                                                </view>
                                            </block>
                                            <block wx:if="{{val.type.amount && val.type.amount.length > 0}}">
                                                <view>
                                                    <view class="box-list-bao" wx:for="{{val.type.amount}}" wx:for-item="value" wx:for-index="_anonIdx6">
                                                        <block wx:if="{{value}}">
                                                            <view>111</view>
                                                        </block>
                                                        <block wx:if="{{value}}">
                                                            <view>222</view>
                                                        </block>
                                                    </view>
                                                </view>
                                            </block>
                                            <block wx:if="{{val.type.borrow}}">
                                                <view class="box-list-bao">{{val.type.borrow}}</view>
                                            </block>
                                        </view>
                                    </block>
                                    <block wx:if="{{val.name != 0}}">
                                        <view>
                                            <view class="box-list-type">
                                                <block wx:if="{{val.type.hour}}">
                                                    <view>
                                                        <block wx:if="{{val.type.hour.num}}">
                                                            <view>{{val.type.hour.num}}</view>
                                                        </block>
                                                        <block>
                                                            <block wx:if="{{val.type.hour.work_time}}">{{val.type.hour.work_time}}</block>
                                                            <block wx:else>
                                                                <block wx:if="{{val.type.hour.overtime}}">
                                                                    <view>
                                                                        <view class="box-list-bao">{{val.type.hour.work_time}}个工</view>
                                                                        <view class="box-list-bao">{{val.type.hour.overtime}}小时</view>
                                                                    </view>
                                                                </block>
                                                            </block>
                                                        </block>
                                                    </view>
                                                </block>
                                                <block wx:if="{{val.type.work}}">
                                                    <view>
                                                        <block wx:if="{{val.type.work.num}}">
                                                            <view>{{val.type.work.num}}</view>
                                                        </block>
                                                        <block>
                                                            <block wx:if="{{val.type.work.work_time}}">{{val.type.work.work_time}}</block>
                                                            <block wx:else>
                                                                <block wx:if="{{val.type.work.overtime}}">
                                                                    <view>
                                                                        <view class="box-list-bao">{{val.type.work.work_time}}个工</view>
                                                                        <view class="box-list-bao">{{val.type.work.overtime}}小时</view>
                                                                    </view>
                                                                </block>
                                                            </block>
                                                        </block>
                                                    </view>
                                                </block>
                                                <block wx:if="{{val.type.amount}}">
                                                    <view>
                                                        <block wx:if="{{val.type.amount.num}}">
                                                            <view>{{val.amount.work.num}}</view>
                                                        </block>
                                                        <block>
                                                            <block wx:if="{{val.type.amount.work_time}}">{{val.type.amount.work_time}}</block>
                                                            <block wx:else>
                                                                <block wx:if="{{val.type.amount.overtime}}">
                                                                    <view>
                                                                        <view class="box-list-bao">{{val.type.amount.unit_num}}</view>
                                                                        <view class="box-list-bao">{{val.type.amount.unit}}</view>
                                                                    </view>
                                                                </block>
                                                            </block>
                                                        </block>
                                                    </view>
                                                </block>
                                                <block wx:if="{{val.type.borrow}}">
                                                    <view class="box-list-bao">{{val.type.borrow.money}}</view>
                                                </block>
                                            </view>
                                        </view>
                                    </block>
                                </view>
                            </block>
                        </view>
                    </view>
                </view>
            </view>
        </view>
    </view>
</block>