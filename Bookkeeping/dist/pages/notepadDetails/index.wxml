<block wx:if="{{$taroCompReady}}">
    <view class="notepadDetails">
        <view class="time">{{data.creatTime}}</view>
        <view class="content">{{data.note}}</view>
        <view class="imageList">
            <view class="image" wx:for="{{loopArray34}}" wx:for-item="v" wx:for-index="__index0">
                <image class="image-image" src="{{v.$original.httpurl}}" bindtap="anonymousFunc0" data-e-tap-so="this" data-e-tap-a-a="{{v._$indexKey}}"></image>
            </view>
        </view>
        <view class="footer">
            <view class="footer-box">
                <view class="footer-del" bindtap="anonymousFunc1">删除</view>
                <view class="footer-btn" bindtap="anonymousFunc2">修改</view>
            </view>
        </view>
    </view>
</block>